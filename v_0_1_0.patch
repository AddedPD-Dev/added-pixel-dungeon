+			target.sprite.add( CharSprite.State.LIGHTENED ); }
+		else if (target.invisible == 0) {
+			target.sprite.remove( CharSprite.State.ILLUMINATED );
+			target.sprite.remove( CharSprite.State.LIGHTENED );
+		}
+	}
