+		} else {
+			return false;
+		}
+	}
+
+	@Override
+	public boolean act() {
+
+		if (target.isAlive()) {
+			partialRegen++;
+
+			if (partialRegen >= 10) {
+				target.HP = Math.min(target.HT, target.HP+1);
+				partialRegen = 0;
+			}
+
+			if (target.HP <= target.HT/4 && !injured) {
+				injured = true;
+				GLog.w( Messages.get(Devotion.class, "baptize_injured", target.name) );
+			} else if (target.HP > target.HT/4) injured = false;
+		}
+
+		spend(TICK);
+		return true;
+	}
